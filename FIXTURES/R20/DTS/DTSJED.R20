4    61   L    6   100   1N   0   1   "CTO"               0     0     0
14    1   L    7   100   1N   0   1   "Colour Macro"      0     0     0
3    61   s    8   100   1N   E   1   "Pan"             127     0     0
0     0   L    9   100   1N   E   1   "Pan Low"         127     0     0
